{
  "expo": {
    "name": "vicin-app",
    "slug": "vicin-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vicinapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourco.vicin",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Vicin uses your location to show nearby posts and events."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
      },
      "package": "com.bkumarvaddineni.vicinapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "deeplinks": [
      {
        "scheme": "vicinapp",
        "path": "oauth-callback",
        "query": {},
        "resetOnTap": true
      }
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9573c6f5-f50d-482d-a6b1-d90ca9d527f9"
      }
    },
    "owner": "bkumarvaddineni"
  }
}
